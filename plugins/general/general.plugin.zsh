# Generic settings and aliases for ZSH
alias reload="source ~/.zshrc"
alias editconf="vim ~/.zshrc"
alias desktop-file-local-install="desktop-file-install --dir=$HOME/.local/share/applications/"
alias nsupdate="rlwrap nsupdate"
alias gitup="git fetch && git pull"
alias quit='exit'
alias q='quit'
alias rssh='ssh -l root'
alias sdig='dig +noall +answer +search'
